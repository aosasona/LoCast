{
	"expo": {
		"name": "YouCast",
		"slug": "YouCast",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icons/light.png",
		"scheme": "youcast",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#151515"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "wyte.space.YouCast"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icons/transparent.png",
				"backgroundColor": "#151515"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "670886a9-74b0-41c6-8f13-7c4d6f7b07d2"
			}
		}
	}
}
